{
  "name": "nfs-react-component-creator-boilerplate",
  "version": "1.0.0",
  "description": "bolier plate startup for creating new react component",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "webpack"
  },
  "author": "Nodeframe Solution Co.,Ltd.",
  "license": "ISC",
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "0.26.1",
    "jest": "^19.0.2",
    "raw-loader": "0.5.1",
    "react-test-renderer": "^15.4.2",
    "style-loader": "0.13.1",
    "webpack": "1.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    }
  }
}
